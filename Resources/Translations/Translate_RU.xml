  <Translate key="Large Health Pack (100) &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;"  translate="Большой Медицинский Набор (100) &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />
  <Translate key="Extraction Tracker &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" translate="Детектор Выгрузки &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />
  <Translate key="Valuable Tracker &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" translate="Детектор Ценностей &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />

  <!-- Upgrades -->

  <Translate key="Sprint Speed Upgrade &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" translate="Улучшение скорости бега &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />
  <Translate key="Map Player Count Upgrade &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" translate="Количество выживших на карте &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />
  <Translate key="Health Upgrade &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" translate="Улучшение здоровья &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />
  <Translate key="Range Upgrade &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" translate="Улучшение дальности &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />
  <Translate key="Tumble Launch Upgrade &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" translate="Улучшение рывка &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />
  <Translate key="Stamina Upgrade &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" translate="Улучшение стамины &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />
  <Translate key="Strength Upgrade &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" translate="Улучшение силы &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />
  <Translate key="Extra Jump Upgrade &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" translate="Улучшение дополнительного прыжка &lt;color=#FFFFFF&gt;[[interact]]&lt;/color&gt;" />

<!-- Tutorial Labels -->
