  <!-- - - - - Tips(FOCUS) - - - - -->
  
  <!-- 1. Movement Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Move to fill the progress bar" translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Передвигайтесь, &lt;br&gt;&lt;br&gt;чтобы заполнить шкалу прогресса" />
  
  <!-- 2. Jump Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Jump to fill the progress bar" translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Прыгайте, &lt;br&gt;&lt;br&gt;чтобы заполнить шкалу прогресса" />
  
  <!-- 3. Crouch Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Sneak to fill the progress bar" translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Присядьте, &lt;br&gt;&lt;br&gt;чтобы заполнить шкалу прогресса" />
  
  <!-- 4. Hide Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Hide to fill the progress bar" translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Прячьтесь, &lt;br&gt;&lt;br&gt;чтобы заполнить шкалу прогресса" />
  
  <!-- 5. Run Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Sprint to fill the progress bar" translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Бегите, &lt;br&gt;&lt;br&gt;чтобы заполнить шкалу прогресса" />
  
  <!-- 6. Throw Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Run, jump and fall to fill the progress bar" translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Бегите, прыгайте и падайте, &lt;br&gt;&lt;br&gt;чтобы заполнить шкалу прогресса" />
  
  <!-- 7. Item grab Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Carry that thing to fill the progress bar" translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Возьмите эту штуку, &lt;br&gt;&lt;br&gt;чтобы заполнить шкалу прогресса" />
  
  <!-- 8. Item distance change Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Alter the distance of a grabbed item" translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Измените дистанцию до предмета" />
  
  <!-- 9. Item rotate Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Rotate stuff to fill the progress bar." translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Вращяйте предметы, &lt;br&gt;&lt;br&gt;чтобы заполнить шкалу прогресса" />
  
  <!-- 10. Item toggle Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Toggle that thing on and off." translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Переключайте эту штуку" />
  
  <!-- 11. Inventory Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Fill your entire inventory." translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Наполните свой инвентарь." />
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Empty your entire inventory." translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Опустошите свой инвентарь." />
  
  <!-- 12. Map Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Check the map." translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Проверьте карту." />
  
  <!-- 13. C.A.R.T. Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Walk around with the C.A.R.T. a little." translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Немного походите вокруг с:&lt;br&gt;Т.Е.Л.Е.Г.А." lineSpacing="5" />
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Fill the C.A.R.T. with valuables." translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Наполните:&lt;br&gt; Т.Е.Л.Е.Г.А. ценностями." lineSpacing="5" />
  
  <!-- 14. Extraction Point Tutorial -->
  
  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Extract the valuables." translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Выгрузите ценности." />
  
  <!-- 15. Truck Tutorial -->

  <Translate key="&lt;b&gt;FOCUS &gt; &lt;/b&gt;Get into the truck and send a message." translate="&lt;b&gt;ВНИМАНИЕ &gt; &lt;/b&gt;Садитесь в грузовик и отправьте сообщение." />
  
  <!-- - - - - Main Labels - - - - -->
  
  <!-- Good job -->
  
  <Translate key="Good job! &lt;sprite name=creepycrying&gt;" translate="Хорошая работа! &lt;sprite name=creepycrying&gt;" />
  
  <!-- 1. Movement Tutorial -->
  
  <Translate key="Use [move] to move." translate="Используйте [move] для передвижения." />
  
  <!-- 2. Jump Tutorial -->
  
  <Translate key="Press [jump] to jump." translate="Нажмите [jump] чтобы прыгать." />
  
  <!-- 3. Crouch Tutorial -->
  
  <Translate key="Press [crouch] to sneak. Monsters are less likely to see you." translate="Нажмите [crouch] чтобы присесть. Монстры с меньшей вероятностью увидят вас." />
  
  <!-- 4. Hide Tutorial -->
  
  <Translate key="Use [crouch] to sneak under stuff and hide." translate="Нажмите [crouch] чтобы прятаться под обьектами." />
  
  <!-- 5. Run Tutorial -->
  
  <Translate key="Hold down [sprint] to sprint. This drains your energy." translate="Удерживайте [sprint] чтобы бежать. Это тратит вашу энергию." />
  
  <!--  6. Throw Tutorial -->
  
  <Translate key="Run and press [tumble] to fall with style. Jumping also helps!" translate="Бегите и нажимайте [tumble] чтобы упасть со стилем. Прыжки так-же помогают!" />
  
  <!-- 7. Item grab Tutorial -->
  
  <Translate key="Grab objects using [grab]." translate="Берите объекты используя [grab]." />
  
  <!-- 8. Item distance change Tutorial -->
  
  <Translate key="Use [push] to alter the distance of grabbed objects." translate="Используйте [push] чтобы изменить расстояние до взятого предмета." />
  
  <!-- 9. Item rotate Tutorial -->
  
  <Translate key="Rotate grabbed objects using [grab2]." translate="Вращяйте взятые объекты используя [grab2]." />
  
  <!-- 10. Item toggle Tutorial -->
  
  <Translate key="Grab and press [interact] to toggle items." translate="Возьмите и нажмите [interact] чтобы включить предметы." />
  
  <!-- 11. Inventory Tutorial -->
  
  <Translate key="Grab an Item and use [inventory1]/[inventory2]/[inventory3] to put it in your inventory." translate="Возьмите предмет и используйте [inventory1]/[inventory2]/[inventory3] чтобы положить его в свой инвентарь." />
  <Translate key="Use [inventory1]/[inventory2]/[inventory3] to take items out of your inventory." translate="Используйте [inventory1]/[inventory2]/[inventory3] чтобы взять предметы из инвентаря."/>

  <!-- 12. Map Tutorial -->

  <Translate key="Use [map] to view the map. The map is &lt;b&gt;&lt;u&gt;important&lt;/b&gt;&lt;/u&gt; for navigation." translate="Используйте [map] чтобы смотреть карту. Карта &lt;b&gt;&lt;u&gt;ВАЖНА&lt;/b&gt;&lt;/u&gt; для навигации." />

  <!-- 13. C.A.R.T. Tutorial -->
  
  <Translate key="This is a C.A.R.T. Grab it by the handle." translate="Это Т.Е.Л.Е.Г.А. , Возьмите ее за ручку." />
  <Translate key="Fill C.A.R.T.s with valuables. C.A.R.T.s are &lt;b&gt;&lt;u&gt;important&lt;/b&gt;&lt;/u&gt; for carrying more stuff." translate="Заполните Т.Е.Л.Е.Г.А. ценностями. Т.Е.Л.Е.Г.А. &lt;b&gt;&lt;u&gt;ВАЖНА&lt;/b&gt;&lt;/u&gt; для перевозки большего количества предметов." />
  
  <!-- 14. Extraction Point Tutorial -->
  
  <Translate key="Activate and fill &lt;b&gt;&lt;u&gt;Extraction Points&lt;/b&gt;&lt;/u&gt; with valuables." translate="Активируйте и заполняйте &lt;b&gt;&lt;u&gt;Точки Выгрузки&lt;/b&gt;&lt;/u&gt; ценностями." />
  
  <!-- 15. Truck Tutorial -->

  <Translate key="Enter the &lt;b&gt;&lt;u&gt;truck&lt;/b&gt;&lt;/u&gt; and send a message to your boss in order to leave a level." translate="Войдите в &lt;b&gt;&lt;u&gt;грузовик&lt;/b&gt;&lt;/u&gt; и отправьте сообщение вашему боссу чтобы покинуть уровень." />
  
  <!-- - - - - Red Tips - - - - -->
  
  <!-- 1. Movement Tutorial -->
  
  <Translate key="Use [move] on your keyboard until the progress bar is filled!" translate="Удерживайте [move] на вашей клавиатуре пока шкала прогрессии не заполниться!" size="12" />

  <!-- 2. Jump Tutorial -->

  <Translate key="Press [jump] to do jumps until the progress bar is filled!" translate="Нажмите [jump] чтобы прыгать пока шкала прогрессии не заполниться!" size="12" />
  
  <!-- 3. Crouch Tutorial -->

  <Translate key="Hold [crouch] and use [move] until the progess is filled!" translate="Удерживайте [crouch] и используйте [move] пока шкала прогрессии не заполниться!" size="12" />

  <!-- 4. Hide Tutorial -->

  <Translate key="Hold [crouch] and walk in under a prop, stay there until you fill the progress bar!" translate="Удерживайте [crouch] и заползите под объект, оставайтесь там пока шкала прогрессии не заполниться!" size="12" />

  <!-- 5. Run Tutorial -->
  
  <Translate key="Hold [sprint] and use [move] to sprint until you filled the progress bar! If the energy runs out, wait for it to regenerate!" translate="Удерживайте [sprint] и используйте [move] чтобы бежать пока шкала прогрессии не заполниться!&lt;br&gt;Если енергия закончиться, дождитесь ее восстановления!" size="12" lineSpacing="5" />

  <!-- 6. Throw Tutorial -->

  <Translate key="Run ([sprint]), jump ([jump]) and press [tumble] to throw yourself. Do it until you fill the progress bar!" translate="Бегите ([sprint]), прыгайте ([jump]) и нажимайте [tumble] чтобы подбросить себя.&lt;br&gt;делайте это пока шкала прогрессии не заполниться!" size="12" lineSpacing="5" />

  <!-- 7. Item grab Tutorial -->

  <Translate key="Grab ([grab]) and hold it until you fill the progress bar!" translate="Возьмите ([grab]) и держите это пока шкала прогрессии не заполниться!" size="12" />

  <!-- 8. Item distance change Tutorial -->

  <Translate key="Grab ([grab]) an item and use [push] to alter the distance of the item you grabbed!" translate="Возьмите ([grab]) предмет и используйте [push],&lt;br&gt;чтобы изменить расстояние до предмета, который вы схватили!" size="12" lineSpacing="5" />

  <!-- 9. Item rotate Tutorial -->

  <Translate key="Grab ([grab]) and hold it while you press [grab2] and move your aim to rotate it. Rotate until you fill the progress bar!" translate="Возьмите ([grab]) и удерживайте его пока нажимаете [grab2] и двигаете свою мышь для его вращения.&lt;br&gt;Вращяйте пока шкала прогрессии не заполниться!" size="10" lineSpacing="5" />

  <!-- 10. Item toggle Tutorial -->
  
  <Translate key="Grab ([grab]) the ball and toggle it on and off using [interact] until you fill the progress bar!" translate="Возьмите ([grab]) мяч и переключайте его используя [interact] пока шкала прогрессии не заполниться!" size="12" />

  <!-- 11. Inventory Tutorial -->

  <Translate key="Grab ([grab]) an item, hold it and press [inventory1]/[inventory2]/[inventory3]. Fill all spots in your inventory to complete the progress bar!" translate="Возьмите ([grab]) предмет, держите его и нажимайте [inventory1]/[inventory2]/[inventory3].&lt;br&gt;Заполните все слоты в вашем инвентаре чтобы заполнить шкалу прогрессии!" size="12" lineSpacing="5"/>
  <Translate key="Use [inventory1]/[inventory2]/[inventory3] and empty all spots to fill the progress bar!" translate="Используйте [inventory1]/[inventory2]/[inventory3] и опустошите все слоты чтобы заполнить шкалу прогресса." size="12" />

  <!-- 12. Map Tutorial -->

  <Translate key="Hold down [map] and view it until you have filled the progress bar!" translate="Зажмите [map] и смотрите карту пока вы не заполните шкалу прогресса!" size="12" />

  <!-- 13. C.A.R.T. Tutorial -->

  <Translate key="Grab ([grab]) the C.A.R.T. by the handle and walk around with it until you have filled the progress bar!" translate="Возьмите ([grab]) Т.Е.Л.Е.Г.А. за ручку и ходите вокруг с ней пока шкала прогресса не заполниться!" size="12" />
  <Translate key="Put three items into the C.A.R.T." translate="Положите три предмета в Т.Е.Л.Е.Г.А." />

  <!-- 14. Extraction Point Tutorial -->
  
  <Translate key="HELLO! FILL THE &lt;b&gt;&lt;u&gt;EXTRACTION POINT&lt;/b&gt;&lt;/u&gt; WITH VALUABLES!" translate="ПРИВЕТ! НАПОЛНИ &lt;b&gt;&lt;u&gt;ТОЧКУ ВЫГРУЗКИ&lt;/b&gt;&lt;/u&gt; ЦЕННОСТЯМИ!" />

  <!-- 15. Truck Tutorial -->

  <Translate key="Walk into the truck, grab ([grab]) and hold the message area on the truck screen to send a message!" translate="Сядьте в грузовик, возьмите ([grab]) и держите область сообщения на экране грузовика для отправки сообщения!" size="12" />

